<div class="status-wrapper py-4 sm:py-15 lg:pb-8 bg-secondary">
  <div class="container mx-auto px-4">
    <div class="boxshadow-none">
      <input id="hiddenInput" type="hidden" value="<%= JSON.stringify(menu) %>">
      <div class="breadcrumbs py-2">
        <div class="inline-block">
          <a
            href="/"
            class="
              text-sm
              inline-block
              align-middle
              breadcrumbs-title
              hover:text-primary
            "
            >Home</a
          >
          <svg
            xmlns="http://www.w3.org/2000/svg"
            fill="none"
            viewBox="0 0 24 24"
            stroke="currentColor"
            class="h-3 w-5 align-middle inline-block"
          >
            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              stroke-width="2"
              d="M9 5l7 7-7 7"
            />
          </svg>
        </div>
        <div class="inline-block">
          <a
            href="/customer/orders"
            class="
              text-sm
              inline-block
              align-middle
              breadcrumbs-title
              hover:text-primary
            "
            >My Orders</a
          >
          <svg
            xmlns="http://www.w3.org/2000/svg"
            fill="none"
            viewBox="0 0 24 24"
            stroke="currentColor"
            class="h-3 w-3 align-middle inline-block order-place-dot"
          >
            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              stroke-width="2"
              d="M9 5l7 7-7 7"
            />
          </svg>
        </div>
        <div class="inline-block">
          <p class="text-sm uppercase"><%=menu._id %></p>
        </div>
      </div>
      <div class="delivery-details order-details bg-white mb-4 mt-2">
        <div class="md:w-3/6 py-4 px-3 xl:py-6 xl:px-6 w-11/12">
          <div class="inline-block">
            <div class="delivery-address font-semibold mb-4">
              <span>Delivery Address</span>
            </div>
            <div class="delivery-name font-semibold text-sm mb-2">
              <span class="customer-billing-name"
                ><%=menu.name %> <%=menu.name
                %></span
              >
            </div>
            <div class="delivery-branch text-sm mb-2">
              <span class="font-semibold customer-branch">Preferred Branch:</span>
              <span class="ml-1">
       
              
              </span>
            </div>
            <div class="delivery-phone text-sm mb-2">
              <span class="font-semibold customer-phoneno">Phone number:</span>
              <span class="ml-1"><%=menu.price%> </span>
            </div>
            <div class="delivery-payement text-sm">
              <span class="font-semibold customer-payment">Payment Type:</span>
              <span class="ml-1 uppercase"><%=menu.price%> </span>
            </div>
          </div>
        </div>
      </div>
      <div class="go-back text-center mt-5">
        <a
          href="/admin/adminMenu"
          class="
            inline-block
            px-3
            py-2
            btn-primary
            rounded-full
            transition
            duration-300
            ease-in-out
            text-white
            shadow
            text-sm
            lg:text-md
            lg:px-6
            lg:py-2.5
          "
          >Go back
        </a>
      </div>
    </div>
  </div>
</div>
