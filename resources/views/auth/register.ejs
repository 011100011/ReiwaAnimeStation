<!-- register section -->
<div class="signup bg-secondary">
  <div class="container px-4 mx-auto">
    <div class="fit-to-screen py-6 flex flex-col items-center justify-center">
      <div class="sign-up flex flex-col bg-white shadow-md px-4 sm:px-6 md:px-8 lg:px-10 py-8 w-full max-w-md">
        <div class="font-medium self-center text-lg lg:text-xl uppercase text-gray-800 mb-2">
          Sign up to create account
        </div>
